extends layout

block content
  h3(class="login_header")= header
  p(class="error")= error
  form(method="post") 
    div(class="form_field")
      p
        label Login 
      input(type="text" name="login")
    div(class="form_field")
      p
        label Password 
      input(type="password" name="password")
    div 
      input(type="submit", value="Submit")